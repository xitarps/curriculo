<p id="notice"><%= notice %></p>

<h1>Career Infos</h1>

<table>
  <thead>
    <tr>
      <th>Company</th>
      <th>Work function</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @career_infos.each do |career_info| %>
      <tr>
        <td><%= career_info.company %></td>
        <td><%= career_info.work_function %></td>
        <td><%= career_info.start_date %></td>
        <td><%= career_info.end_date %></td>
        <td><%= career_info.description %></td>
        <td><%= link_to 'Show', career_info %></td>
        <td><%= link_to 'Edit', edit_career_info_path(career_info) %></td>
        <td><%= link_to 'Destroy', career_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Career Info', new_career_info_path %>
